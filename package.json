{
	"name": "banking-providers-skeleton",
	"short": "bnkc-fnb",
	"version": "0.0.14",
	"description": "Skeleton Provider implementation for Banking Service",
	"main": "handler.js",
	"scripts": {
		"prepenv": "bash ./pipelines/scripts/prepenv.sh -n \"6.14.1\" -l \"3.16.4\" -s \"1.30.3\" -r \"3.0.0\"",
		"clean": "rimraf ./build && rimraf ./coverage && rimraf ./.nyc_output && rimraf node_modules && lerna run clean",
		"rebuild": "npm run clean && npm run build",
		"hoist": "lerna bootstrap --hoist --no-hoist=@sage/bnkc-fnb* --no-hoist=redoc",
		"prebuild": "npm run hoist && lerna run buildX --scope=\"@sage/bnkc-fnb-util-build-swagger\" ",
		"build": "lerna run build",
		"changed/build": "lerna run buildX --scope=\"@sage/bnkc-fnb-util-build-swagger\" && bash ./scripts/lerna_modified.sh build",
		"changed/build:hoist": "npm run hoist && lerna run buildX --scope=\"@sage/bnkc-fnb-util-build-swagger\" && bash ./scripts/lerna_modified.sh build",
		"changed/unit": "bash ./scripts/lerna_modified.sh test",
		"unlock": "rimraf package-lock.json && lerna run unlock",
		"test/unit": "NODE_ENV=test nyc mocha --reporter=mocha-junit-reporter --reporter-options mochaFile=./coverage/junit_unit_results.xml --colors \"./test/unit/**/*.spec.js\"",
		"test/unit:report": "npm run test/unit && open coverage/lcov-report/index.html",
		"test/regression": "NODE_ENV=test nyc mocha --reporter=mocha-junit-reporter --reporter-options mochaFile=./coverage/junit_regression_results.xml --colors \"./test/regression/**/*.spec.js\"",
		"test/regression:report": "npm run test/unit && open coverage/lcov-report/index.html",
		"lint": "eslint ./packages/**",
		"lint/byError": "eslint ./packages/** --format ./node_modules/eslint-stats/byError.js",
		"lint/byWarning": "eslint ./packages/** --format ./node_modules/eslint-stats/byWarning.js",
		"lint/byErrorAndWarning": "eslint ./packages/** --format ./node_modules/eslint-stats/byErrorAndWarningStacked.js",
		"lint/byFolder": "eslint ./packages/** --format ./node_modules/eslint-stats/byFolder.js",
		"docker/redoc": "docker run -it --rm -p 80:80 -v $(pwd)/build/packages:/usr/share/nginx/html/packages -e SPEC_URL=packages/swagger.yaml redocly/redoc",
		"release:ci": "lerna publish from-git --registry=https://sage.myget.org/F/npm/npm/ --yes",
		"release:local": "lerna publish from-git --registry=https://sage.myget.org/F/npm/npm/",
		"version:preBuild": "bash ./scripts/version.sh",
		"version:postBuild": "bash ./scripts/version_commit.sh",
		"version:deployOnly": "bash ./scripts/version_detect.sh",
		"deploy/isPull": "bash ./scripts/is_pull_request.sh",
		"deploy/commitFlags": "bash ./scripts/commit_flags_detect.sh",
		"deploy/environment": "bash ./scripts/deploy_environment.sh ",
		"deploy/build": "mkdir -p ./build/deploy && cp -R ./deploy ./build",
		"mm:status": "lerna run mm:status --scope=@sage/bnkc-fnb-util-deploy-lambda-db-migration",
		"mm:up": "lerna run mm:up --scope=@sage/bnkc-fnb-util-deploy-lambda-db-migration",
		"mm:down": "lerna run mm:down --scope=@sage/bnkc-fnb-util-deploy-lambda-db-migration"
	},
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@babel/cli": "7.5.0",
		"@babel/core": "7.5.4",
		"@babel/plugin-proposal-class-properties": "7.5.0",
		"@babel/preset-env": "7.5.4",
		"@babel/preset-flow": "7.0.0",
		"@babel/register": "7.4.4",
		"aws-sdk": "2.220.1",
		"babel-eslint": "10.0.2",
		"babel-plugin-istanbul": "5.1.4",
		"babel-plugin-transform-async-to-promises": "0.8.12",
		"bluebird": "3.5.1",
		"copy-webpack-plugin": "5.1.1",
		"emitter": "0.0.5",
		"eslint": "4.11.0",
		"eslint-config-airbnb": "16.1.0",
		"eslint-plugin-import": "2.8.0",
		"eslint-stats": "1.0.0",
		"istanbul": "^1.1.0-alpha.1",
		"keypair": "1.0.1",
		"mocha": "4.0.1",
		"mongodb": "3.0.5",
		"nyc": "12.0.2",
		"serverless-webpack": "4.2.0",
		"should": "13.2.1",
		"sinon": "5.0.10",
		"underscore": "1.9.0",
		"webpack": "4.41.2",
		"webpack-node-externals": "1.6.0",
		"mocha-junit-reporter": "1.23.1"
	}
}
